<?xml version="1.0"?>
<cache-config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.oracle.com/coherence/coherence-cache-config" xsi:schemaLocation="http://xmlns.oracle.com/coherence/coherence-cache-config coherence-cache-config.xsd">
    <scope-name>telefonica.coherence.servicio</scope-name>
    <caching-scheme-mapping>
        <cache-mapping>
            <cache-name>CACHE_PRES_BAJA_*</cache-name>
            <scheme-name>TELCO-distributed-baja</scheme-name>
            <init-params>
                <init-param>
                    <param-name>expiry-delay</param-name>
                    <param-value>60s</param-value>
                </init-param>
            </init-params>
        </cache-mapping>
        <cache-mapping>
            <cache-name>CACHE_PRES_MEDIA_*</cache-name>
            <scheme-name>TELCO-distributed-media</scheme-name>
            <init-params>
                <init-param>
                    <param-name>expiry-delay</param-name>
                    <param-value>30m</param-value>
                </init-param>
            </init-params>
        </cache-mapping>
        <cache-mapping>
            <cache-name>CACHE_PRES_ALTA_*</cache-name>
            <scheme-name>TELCO-distributed-alta</scheme-name>
            <init-params>
                <init-param>
                    <param-name>expiry-delay</param-name>
                    <param-value>1h</param-value>
                </init-param>
            </init-params>
        </cache-mapping>
        <cache-mapping>
            <cache-name>CACHE_NUC_BAJA_*</cache-name>
            <scheme-name>TELCO-distributed-baja</scheme-name>
            <init-params>
                <init-param>
                    <param-name>expiry-delay</param-name>
                    <param-value>60s</param-value>
                </init-param>
            </init-params>
        </cache-mapping>
        <cache-mapping>
            <cache-name>CACHE_NUC_MEDIA_*</cache-name>
            <scheme-name>TELCO-distributed-media</scheme-name>
            <init-params>
                <init-param>
                    <param-name>expiry-delay</param-name>
                    <param-value>30m</param-value>
                </init-param>
            </init-params>
        </cache-mapping>
        <cache-mapping>
            <cache-name>CACHE_NUC_ALTA_*</cache-name>
            <scheme-name>TELCO-distributed-alta</scheme-name>
            <init-params>
                <init-param>
                    <param-name>expiry-delay</param-name>
                    <param-value>1h</param-value>
                </init-param>
            </init-params>
        </cache-mapping>
        <cache-mapping>
            <cache-name>T3_FMK_*</cache-name>
            <scheme-name>TELCO-distributed</scheme-name>
            <init-params>
                <init-param>
                    <param-name>expiry-delay</param-name>
                    <param-value>30s</param-value>
                </init-param>
            </init-params>
        </cache-mapping>
        <!-- CachÃ© a nivel de servicio -->
        <cache-mapping>
            <cache-name>CACHE_APP_*</cache-name>
            <scheme-name>TELCO-distributed-app</scheme-name>
            <init-params>
                <init-param>
                    <param-name>expiry-delay</param-name>
                    <param-value>60s</param-value>
                </init-param>
            </init-params>
        </cache-mapping>
    </caching-scheme-mapping>
    <caching-schemes>
        <!-- Proxy config  -->
        <proxy-scheme>
            <scheme-name>COHI-proxy-servicio</scheme-name>
            <service-name>COHI-ExtendTcpProxyService-SERVICIO</service-name>
            <thread-count-min>16</thread-count-min>
            <acceptor-config>
                <tcp-acceptor>
                    <local-address>
                        <address system-property="movistar.coherence.proxyaddress"/>
                        <port system-property="movistar.coherence.proxyport.servicio">1</port>
                    </local-address>
                </tcp-acceptor>
            </acceptor-config>
            <proxy-config>
                <cache-service-proxy>
                    <lock-enabled>true</lock-enabled>
                </cache-service-proxy>
            </proxy-config>
            <!-- Only true for Proxy roles-->
            <autostart system-property="movistar.coherence.proxyenabled.servicio">false</autostart>
        </proxy-scheme>
        <distributed-scheme>
            <scheme-name>TELCO-distributed</scheme-name>
            <service-name>TELCO-DistributedCache</service-name>
            <thread-count-min>4</thread-count-min>
            <lease-granularity>member</lease-granularity>
            <backing-map-scheme>
                <local-scheme>
                    <eviction-policy>LRU</eviction-policy>
                    <high-units>{back-size-limit 256}</high-units>
                    <unit-calculator>BINARY</unit-calculator>
                    <unit-factor>1048576</unit-factor>
                    <expiry-delay>{expiry-delay}</expiry-delay>
                </local-scheme>
            </backing-map-scheme>
            <autostart system-property="movistar.coherence.cacheenabled.servicio">false</autostart>
        </distributed-scheme>
        <distributed-scheme>
            <scheme-name>TELCO-distributed-alta</scheme-name>
            <service-name>TELCO-DistributedCache-ALTA</service-name>
            <thread-count-min>4</thread-count-min>
            <lease-granularity>member</lease-granularity>
            <backing-map-scheme>
                <local-scheme>
                    <eviction-policy>LRU</eviction-policy>
                    <high-units>{back-size-limit 256}</high-units>
                    <unit-calculator>BINARY</unit-calculator>
                    <unit-factor>1048576</unit-factor>
                    <expiry-delay>{expiry-delay}</expiry-delay>
                </local-scheme>
            </backing-map-scheme>
            <autostart system-property="movistar.coherence.cacheenabled.servicio">false</autostart>
        </distributed-scheme>
        <distributed-scheme>
            <scheme-name>TELCO-distributed-media</scheme-name>
            <service-name>TELCO-DistributedCache-MEDIA</service-name>
            <thread-count-min>4</thread-count-min>
            <lease-granularity>member</lease-granularity>
            <backing-map-scheme>
                <local-scheme>
                    <eviction-policy>LRU</eviction-policy>
                    <high-units>{back-size-limit 256}</high-units>
                    <unit-calculator>BINARY</unit-calculator>
                    <unit-factor>1048576</unit-factor>
                    <expiry-delay>{expiry-delay}</expiry-delay>
                </local-scheme>
            </backing-map-scheme>
            <autostart system-property="movistar.coherence.cacheenabled.servicio">false</autostart>
        </distributed-scheme>
        <distributed-scheme>
            <scheme-name>TELCO-distributed-baja</scheme-name>
            <service-name>TELCO-DistributedCache-BAJA</service-name>
            <thread-count-min>4</thread-count-min>
            <lease-granularity>member</lease-granularity>
            <backing-map-scheme>
                <local-scheme>
                    <eviction-policy>LRU</eviction-policy>
                    <high-units>{back-size-limit 256}</high-units>
                    <unit-calculator>BINARY</unit-calculator>
                    <unit-factor>1048576</unit-factor>
                    <expiry-delay>{expiry-delay}</expiry-delay>
                </local-scheme>
            </backing-map-scheme>
            <autostart system-property="movistar.coherence.cacheenabled.servicio">false</autostart>
        </distributed-scheme>
        <distributed-scheme>
            <scheme-name>TELCO-distributed-app</scheme-name>
            <service-name>TELCO-DistributedCache-APP</service-name>
            <thread-count-min>4</thread-count-min>
            <lease-granularity>member</lease-granularity>
            <backing-map-scheme>
                <local-scheme>
                    <eviction-policy>LRU</eviction-policy>
                    <high-units>{back-size-limit 256}</high-units>
                    <unit-calculator>BINARY</unit-calculator>
                    <unit-factor>1048576</unit-factor>
                    <expiry-delay>{expiry-delay}</expiry-delay>
                </local-scheme>
            </backing-map-scheme>
            <autostart system-property="movistar.coherence.cacheenabled.servicio">false</autostart>
        </distributed-scheme>
    </caching-schemes>
</cache-config>